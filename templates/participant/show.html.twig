

{% block title %}Participant{% endblock %}

{% block body %}
    <h1>Participant</h1>
 {# {{ include('participant/_delete_form.html.twig') }} #}
    <table class="table">
        <tbody>
        {% for participant in  participants %}  
        <a href="{{ path('app_participant_index') }}" class="btn">back to list</a>
        <a href="{{ path('app_participant_edit', {'id': participant.id}) }}" class="btn">edit</a>
            <tr>
                <th>Id</th>
                <td>{{ participant.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ participant.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ participant.email }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>

  

   
{% endblock %}
